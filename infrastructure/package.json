{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk --profile tp-access",
    "cdk:diff": "cdk diff --profile tp-access",
    "deploy:all": "yarn build && cdk --profile tp-access deploy Tp*",
    "deploy:gql": "yarn build && cdk --profile tp-access deploy TpGqlStaging"
  },
  "devDependencies": {
    "@types/node": "12.12.8",
    "aws-cdk": "^1.16.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.16.3",
    "@aws-cdk/aws-cloudfront": "^1.16.3",
    "@aws-cdk/aws-ec2": "^1.16.3",
    "@aws-cdk/aws-ecr": "^1.16.3",
    "@aws-cdk/aws-ecs": "^1.16.3",
    "@aws-cdk/aws-ecs-patterns": "^1.16.3",
    "@aws-cdk/aws-iam": "^1.16.3",
    "@aws-cdk/aws-route53": "^1.16.3",
    "@aws-cdk/aws-route53-targets": "^1.16.3",
    "@aws-cdk/aws-s3": "^1.16.3",
    "@aws-cdk/aws-ssm": "^1.16.3",
    "@aws-cdk/core": "^1.16.3",
    "source-map-support": "^0.5.9"
  }
}
